{
  "name": "treinkaart",
  "version": "1",
  "type": "module",
  "scripts": {
    "tsc": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "dev": "python -m http.server",
    "commit": "yarn visualbp && git add script.min.js && git commit",
    "visual": "NODE_ENV=development esbuild script.ts --bundle --outfile=script.js --format=iife --watch",
    "visualb": "NODE_ENV=development esbuild script.ts --bundle --outfile=script.js --format=iife",
    "visualbp": "esbuild script.ts --bundle --outfile=script.min.js --format=iife --minify"
  },
  "engines": {
    "node": ">=20.18.1"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo-google-fonts/atkinson-hyperlegible": "^0.4.0",
    "@expo-google-fonts/inter": "^0.4.1",
    "maplibre-gl": "^5.6.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/jest": "^30.0.0",
    "esbuild": "^0.25.6",
    "eslint-plugin-no-restricted-imports": "^0.0.0",
    "jest": "^29.2.1",
    "jest-expo": "~52.0.6",
    "react-test-renderer": "18.2.0",
    "typescript": "^5.8.3"
  },
  "private": true
